#导入numpy的好处
import numpy as np
def 数组转向架(n):
    a=np.arange(1,n+1)**3
    b=np.arange(1,n+1)**2
    return a+b
print(数组转向架(3))


zyj随机数，两行四列
a=np.random.randn(2,4)


zyjnumpy索引
a=np.arange(20).reshape(4,5)
print(a[0:2,2:4])
print(a[:,2])


zyj筛选
筛选=a>5
print(筛选)
print(a[筛选])


##找出偶数或者小于七的数
条件=(a%2==0)|(a<7)
print(a[条件])

